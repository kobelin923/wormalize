{
  "extends": "rackt",
  "rules": {
    "no-console": 0,
    "arrow-parens": 2,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2,
    "indent": ["error", 2, { "SwitchCase": 0 }],
    "array-bracket-spacing": ["error", "never"]
  },
  "plugins": [
    "react"
  ]
}
